<div class="table-responsive">
  <table class="table table-hover table-striped">
    <thead>
      <tr>
        <th>Name</th>
        <th>RUc</th>
      </tr>
    </thead>
    <tbody>
      <% clients.each do |client| %>
        <tr>
          <td><%= client.name %></td>
          <td><%= client.ruc %></td>
          <td>
            <%= link_to '<span class="glyphicon glyphicon-pencil"></span>'.html_safe, "#{}", class: "btn btn-default btn-sm", data: { "toggle" => "tooltip" }, title: "Edit" %>
          </td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
