<div class="table-responsive">
  <table class="table table-hover table-striped">
    <thead>
      <tr>
        <th>code</th>
        <th>Description</th>
      </tr>
    </thead>
    <tbody>
      <% inventories.each do |inventory| %>
        <tr>
          <td><%= inventory.code %></td>
          <td><%= inventory.description %></td>
          <td>
            <%= link_to '<span class="glyphicon glyphicon-pencil"></span>'.html_safe, "#{}", class: "btn btn-default btn-sm", data: { "toggle" => "tooltip" }, title: "Edit" %>
          </td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
